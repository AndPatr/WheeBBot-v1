<?xml version="1.0"?>
<!-- used to circumvent the problem which arises because Gazebo cannot use the package:// spec. -->
<model> 
  <name>wheebbot</name>
  <version>0.0.0</version>
  <sdf version='1.7'>model.sdf</sdf>

  <author>
   <name>Andrea Patrizi</name>
   <email>andrea.patrizi1b6e6@gmail.com</email>
  </author>

  <description>
    Hack model.config used only to be able to use the package:// directory into the URDF. 
    This works because the URDF gazebo parser replaces package:// with model://
  </description>
</model>
